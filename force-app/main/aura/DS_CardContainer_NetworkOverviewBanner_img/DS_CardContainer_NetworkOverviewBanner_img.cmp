<aura:component controller='DS_NetworkOverviewBanner' implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global" >
<aura:handler name="init" value="{!this}" action="{!c.doInit}" />
<aura:attribute name="contents" type="String" default="" />
    
	<lightning:layoutItem size="12" class="greyBoxCard networkBanner">
        <lightning:layout multipleRows="true">
            <lightning:layoutItem size="12" class="customer-network-img">
                <figure>
   					<img src="{!v.contents}" />
				</figure>
            </lightning:layoutItem>
            <!-- lightning:layoutItem class="" flexibility="grow" size="12">
                <p><a href="">View Report</a></p>
            </lightning:layoutItem -->
        </lightning:layout>
 	</lightning:layoutItem>
    
    
    
</aura:component>