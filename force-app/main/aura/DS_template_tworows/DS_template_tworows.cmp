<aura:component implements="forceCommunity:layout" description="Custom Content Layout" access="global">
    <aura:attribute name="columnrowone" type="Aura.Component[]" required="false" />
    <aura:attribute name="columnrowtwo" type="Aura.Component[]" required="false" />
	
    <lightning:layoutItem size="12" class="page-content" padding="around-small">
    	<lightning:layout multipleRows="true">
    		<lightning:layoutItem size="12" class="page-content-rowone">
        		{!v.columnrowone}
        	</lightning:layoutItem>
            <br />
            <lightning:layoutItem size="12" class="page-content-rowtwo">
        		{!v.columnrowtwo}	
        	</lightning:layoutItem>
    	</lightning:layout>
    </lightning:layoutItem>
           
</aura:component>