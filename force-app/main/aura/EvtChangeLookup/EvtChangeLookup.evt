<aura:event type="APPLICATION" description="Lookup field is updated" >
	<aura:attribute name="name" type="String" description="Name of source lookup field" required="false"/>
	<aura:attribute name="record" type="Object" description="SObject record of the new field value" required="false"/>
	<aura:attribute name="recordId" type="String" description="Value of the new field value" required="true"/>
	<aura:attribute name="recordName" type="String" description="SObject record name of the new field value" required="true"/>
    <!--aura:attribute name="isEmptyField" type="boolean"/-->
</aura:event>