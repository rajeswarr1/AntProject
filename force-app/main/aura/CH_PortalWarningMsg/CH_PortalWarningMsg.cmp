<aura:component implements="forceCommunity:availableForAllPageTypes,force:hasRecordId" access="global" controller="CH_LightningController">
	<aura:attribute name="recordId" type="Id"/>
    <aura:attribute name="isPendingCustomer" type="Boolean" default="false"/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <aura:if isTrue="{!v.isPendingCustomer}">
        <div class="slds-box slds-theme_default">
            <label class="error_msg">{!$Label.c.CH_Msg_PendingCustomer_Warning}</label>
        </div>
    </aura:if>
</aura:component>