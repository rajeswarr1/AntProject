<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction,lightning:isUrlAddressable" access="global" >
	
    <aura:attribute name="recordsId" type="String"/>
    <aura:attribute name="ccrURI" type="String"/>
    <lightning:workspaceAPI aura:id="workspace"/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <iframe width="100%" height="100%"  src="{!v.ccrURI}" frameborder="0" sandbox="allow-popups  allow-popups-to-escape-sandbox allow-same-origin allow-scripts allow-forms"></iframe>
    
</aura:component>