<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,force:lightningQuickAction" access="global" controller="ApprovalCentreController">    <aura:attribute name="recordId" type="String" description="Opportunity Record Id" default="{!v.recordId}"/>
    <aura:attribute name="approvalCenter" type="List"/>
    
    
    <aura:attribute name="wrapper" type="ApprovalCentreWrapper" />  
    <aura:attribute name="DocWrapper" type="DocumentWrapper" />
     <aura:attribute name="G4G5DocWrapper" type="DocumentWrapper" />
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <aura:handler event="force:refreshView" action="{!c.doInit}" />
    <aura:attribute name="modalPopUp" type="boolean" default="false"/>
    <aura:attribute name="vfURL" type="String" default="false"/>
    <aura:attribute name="userPicURL" type="String" default="/resource/SldsNew/assets/images/avatar3.jpg"/>
    <aura:attribute name="documents" type="Map" />
    <aura:attribute name="ConfiDocument" type="Map" />
    <aura:attribute name="G4_G5_Documents" type="ContentVersion" />
    <aura:attribute name="strAppComments" type="string" default=""/>
    <aura:attribute name="actiontype" type="string" default=""/>
    <aura:attribute name="OfferOutput" type="Offer__c" default="{'sobjectType': 'Offer__c', 'Offer_Description__c': '',
                                                                 'Approval_Scope__c' :'' }"/>
    <aura:attribute name="ApprovalReq" type="Apttus_Approval__Approval_Request__c" default="{'sobjectType': 'Apttus_Approval__Approval_Request__c',
                                                                                            'Apttus_Approval__Step_Name__c': '', 'Apttus_Approval__Approval_Status__c' :'','Apttus_Approval__Assigned_To_Name__c':'','NF_Assignee_Type__c':'','Apttus_Approval__DelegateApprover__c':'','NF_Role_Name__c':'','NF_Organization_Level__c':'','Apttus_Approval__ActualApproverName__c':'','Apttus_Approval__Approver_Comments__c':''}"/>  
    <aura:attribute name="approvalList" type="List" />
    <aura:attribute name="mobileDevice" type="boolean" default="false" description="True if component is rendered in mobile"/>
    <aura:attribute name="ApproverDelegatePopUp" type="boolean" default="false"/>
    <aura:attribute name="ValidatorDelegatePopUp" type="boolean" default="false"/>
    <aura:attribute name="Spinner" type="boolean" default="false"/>
    <aura:attribute name="loggedinUserId" type="string" default=""/>
    <aura:attribute name="validatorDelgateCount" type="integer" default=""/>
    <aura:attribute name="approverDelgateCount" type="integer" default=""/>
    <aura:if isTrue="{!v.Spinner}">
        <div aura:id="spinnerId" class="slds-spinner_container">
            <div class="slds-spinner--brand  slds-spinner slds-spinner--large slds-is-relative" role="alert">
                <span class="slds-assistive-text">Loading</span>
                <div class="slds-spinner__dot-a"></div>
                <div class="slds-spinner__dot-b"></div>
            </div>
        </div>
    </aura:if>
    <aura:if isTrue="{!and(v.ApproverDelegatePopUp,v.wrapper.type == ' Approving as Delegate')}">
        <div role="dialog" tabindex="-1" aria-labelledby="header99" class="slds-modal slds-fade-in-open ">
            <div class="slds-modal__container">
                <!-- ###### MODAL BOX HEADER Part Start From Here ######-->
                <div class="slds-modal__header">
                    <button class="slds-button slds-modal__close slds-button--icon-inverse" title="Close" onclick="{!c.closeDelegateModel}">
                        X
                        <span class="slds-assistive-text">Close</span>
                    </button>                    
                    <h2 id="header99" class="slds-text-heading--medium">Please select a approver</h2>
                </div>   
                <div class="slds-modal__content slds-scrollable slds-p-around--medium">					
                    <aura:if isTrue="{!!empty(v.wrapper.approverRecList)}"> 			                        
                        <table class="slds-table slds-table_bordered slds-table_cell-buffer slds-table_col-bordered">  
                            <thead>
                                <tr class="slds-text-title_caps">
                                    <th scope="col">
                                        <div class="slds-truncate" title="SELECT">SELECT</div>
                                    </th>
                                    <th scope="col">
                                        <div class="slds-truncate" title="ROLE Name">ROLE NAME</div>
                                    </th>
                                    <th scope="col">
                                        <div class="slds-truncate" title="ASSIGNED TO">ASSIGNED TO</div>
                                    </th>
                                    <th scope="col">
                                        <div class="slds-truncate" title="ACTUAL APPROVER">ACTUAL APPROVER</div>
                                    </th>
                                    <th scope="col">
                                        <div class="slds-truncate" title="APPROVAL STATUS">APPROVAL STATUS</div>
                                    </th>      
                                </tr>
                            </thead>                                
                            <tbody>     
                                <aura:iteration items="{!v.wrapper.approverRecList}" var="approvalRec">
                                    <aura:if isTrue="{!and(or(approvalRec.Apttus_Approval__Approval_Status__c == 'Assigned',approvalRec.Apttus_Approval__Approval_Status__c == 'Reassigned'),approvalRec.Apttus_Approval__DelegateApproverIds__c == v.loggedinUserId)}">
                                        <tr>
                                            <td data-label="SELECT">                                            
                                                <ui:inputCheckbox labelClass="" text="{!approvalRec.Id}" aura:id="ApprovalRecCheckbox" value="" change="{!c.delegateCheckAction}"/>
                                            </td>
                                            <td data-label="ROLE Name">
                                                <div class="slds-truncate" title="{!approvalRec.NF_Role_Name__c}">{!approvalRec.NF_Role_Name__c}</div>
                                            </td>                                    
                                            <td data-label="ASSIGNED TO">
                                                <div class="slds-truncate" title="{!approvalRec.Apttus_Approval__Assigned_To_Name__c}">{!approvalRec.Apttus_Approval__Assigned_To_Name__c}</div>
                                            </td>                                    
                                            <td data-label="ACTUAL APPROVER">
                                                <div class="slds-truncate" title="{!approvalRec.Apttus_Approval__ActualApproverName__c}">{!approvalRec.Apttus_Approval__ActualApproverName__c}</div>
                                            </td>                                                                                                              
                                            <td data-label="APPROVAL STATUS">
                                                <div class="slds-truncate" title="{!approvalRec.Apttus_Approval__Approval_Status__c}">{!approvalRec.Apttus_Approval__Approval_Status__c}</div>
                                            </td>                                                                            
                                        </tr>
                                    </aura:if>
                                </aura:iteration>
                            </tbody>
                        </table>                                                   		
                    </aura:if>
                </div>  
                <!--###### MODAL BOX FOOTER Part Start From Here ######-->
                <div class="slds-modal__footer">
                    
                </div>
            </div>
        </div>
        <div class="slds-backdrop slds-backdrop--open"></div>
    </aura:if> 
    <aura:if isTrue="{!and(v.ValidatorDelegatePopUp,v.wrapper.type == 'Validator as Delegate')}">    
        <div role="dialog" tabindex="-1" aria-labelledby="header99" class="slds-modal slds-fade-in-open ">
            <div class="slds-modal__container">
                <!-- ###### MODAL BOX HEADER Part Start From Here ######-->
                <div class="slds-modal__header">
                    <button class="slds-button slds-modal__close slds-button--icon-inverse" title="Close" onclick="{!c.closeDelegateModel}">
                        X
                        <span class="slds-assistive-text">Close</span>
                    </button>                    
                    <h2 id="header99" class="slds-text-heading--medium">Please select a validator</h2>
                </div>   
                <div class="slds-modal__content slds-p-around--medium">					
                    <aura:if isTrue="{!!empty(v.wrapper.validatorRecList)}">                                                     
                        <table class="slds-table slds-table_bordered slds-table_cell-buffer slds-table_col-bordered">
                            <thead>
                                <tr class="slds-text-title_caps">
                                    <th scope="col">
                                        <div class="slds-truncate" title="SELECT">SELECT</div>
                                    </th>
                                    <th scope="col">
                                        <div class="slds-truncate" title="ROLE Name">ROLE NAME</div>
                                    </th>
                                    <th scope="col">
                                        <div class="slds-truncate" title="ASSIGNED TO">ASSIGNED TO</div>
                                    </th>
                                    <th scope="col">
                                        <div class="slds-truncate" title="ACTUAL APPROVER">ACTUAL VALIDATOR</div>
                                    </th>
                                    <th scope="col">
                                        <div class="slds-truncate" title="APPROVAL STATUS">APPROVAL STATUS</div>
                                    </th>      
                                </tr>
                            </thead> 
                            <tbody>   
                                <aura:iteration items="{!v.wrapper.validatorRecList}" var="approvalRec">
                                    <aura:if isTrue="{!and(empty(approvalRec.Validator_Status__c),approvalRec.Apttus_Approval__DelegateApproverIds__c == v.loggedinUserId)}">
                                        <tr>
                                            <td data-label="SELECT">
                                                <ui:inputCheckbox labelClass="" text="{!approvalRec.Id}" aura:id="ValidatorRecCheckbox" value="" change="{!c.delegateCheckAction}"/>
                                            </td>
                                            <td data-label="ROLE Name">
                                                <div class="slds-truncate" title="{!approvalRec.NF_Role_Name__c}">{!approvalRec.NF_Role_Name__c}</div>
                                            </td>                                    
                                            <td data-label="ASSIGNED TO">
                                                <div class="slds-truncate" title="{!approvalRec.Apttus_Approval__Assigned_To_Name__c}">{!approvalRec.Apttus_Approval__Assigned_To_Name__c}</div>
                                            </td>                                    
                                            <td data-label="ACTUAL VALIDATOR">
                                                <div class="slds-truncate" title="{!approvalRec.Apttus_Approval__ActualApproverName__c}">{!approvalRec.Apttus_Approval__ActualApproverName__c}</div>
                                            </td>                                                                                                              
                                            <td data-label="APPROVAL STATUS">
                                                <div class="slds-truncate" title="{!approvalRec.Apttus_Approval__Approval_Status__c}">{!approvalRec.Apttus_Approval__Approval_Status__c}</div>
                                            </td>  
                                        </tr>
                                    </aura:if>
                                </aura:iteration>
                            </tbody>
                        </table>                                                                                                
                    </aura:if>
                </div>  
                <!--###### MODAL BOX FOOTER Part Start From Here ######-->
                <div class="slds-modal__footer">
                    
                </div>
            </div>
        </div>
        <div class="slds-backdrop slds-backdrop--open"></div>
    </aura:if> 
    <aura:if isTrue="{!v.mobileDevice}">
        <article class="slds-card">
            <div class="slds-box">
                <div class="slds-card__header slds-grid">
                    <header class="slds-media slds-media_center slds-has-flexi-truncate">
                        <div class="slds-media__figure">	  
                            <span class="slds-icon_container slds-icon-standard-contact" title="description of icon when needed">
                                <c:svgIcon svgPath="/resource/SldsNew/assets/icons/standard-sprite/svg/symbols.svg#approval" class="slds-icon slds-icon_small"/>                                                                         
                            </span>
                        </div>
                        <div class="slds-media__body">
                            <h2>
                                <span class="slds-text-heading_small">Approval Center</span>          
                            </h2>
                        </div>
                    </header>   
                    <!--  <aura:if isTrue="{!v.wrapper.stakeHolderComment}">
                        <span class="slds-text-heading_small">{!v.wrapper.type}</span>
                        <lightning:button aura:id="stakeHolderAddCmtBtn" class="slds-button" variant="brand" value="{!v.wrapper.approvalRequestId}" onclick="{!c.validateOrRejectController}" label="Add Comment"/>
                        <aura:set attribute="else"> -->
                    <aura:if isTrue="{!v.wrapper.validatorOrApprover}">
                        <span class="slds-text-heading_small">{!v.wrapper.type}</span>
                        <lightning:button aura:id="validate" disabled="{!!v.wrapper.isApprovalRequested}" class="slds-button" variant="brand"  value="{!v.wrapper.approvalRequestId}"  label="Validate" onclick="{!c.validateOrRejectController}"/>
                        <lightning:button aura:id="ValidateReject" disabled="{!!v.wrapper.isApprovalRequested}" class="slds-button" variant="destructive" value="{!v.wrapper.approvalRequestId}"  label="Reject" onclick="{!c.validateOrRejectController}"/>                        
                        <aura:set attribute="else">
                            <div data-recId ="{!v.wrapper.opportunityId}" class="slds-no-flex">
                                <span class="slds-text-heading_small">{!v.wrapper.type}</span>
                                <lightning:button aura:id="approveButton" class="slds-button" variant="brand" value="{!v.wrapper.approvalRequestId}" onclick="{!c.approvalRequestController}" label="Approve"/>
                                <lightning:button aura:id="rejectButton" class="slds-button" variant="destructive" value="{!v.wrapper.approvalRequestId}" onclick="{!c.rejectionRequestController}" label="Reject"/>                        
                            </div>
                        </aura:set>
                    </aura:if> 
                    <!--    </aura:set>
                    </aura:if>   -->                                    
                </div>            
                <div class="slds-card__body">
                    <table class="slds-table slds-table_bordered slds-no-row-hover slds-table_cell-buffer">           
                        <tbody>            
                            <tr>
                                <th scope="row">
                                    <div class="slds-truncate" title="Gate Step">                        
                                        <div class="slds-form-element">
                                            <span class="slds-form-element__label">Gate Step</span>
                                            <div class="slds-form-element__control slds-border_bottom">
                                                <span class="slds-form-element__static">{!v.wrapper.gateStep}</span>
                                            </div>
                                        </div>
                                    </div>
                                </th>
                                <th scope="row">
                                    <div class="slds-truncate" title="Approval request date">                        
                                        <div class="slds-form-element">
                                            <span class="slds-form-element__label">Approval Request Date</span>
                                            <div class="slds-form-element__control slds-border_bottom">
                                                <span class="slds-form-element__static">{!v.wrapper.approvalReqDate}</span>
                                            </div>
                                        </div>
                                    </div>
                                </th>
                                
                                <td>   
                                    <div class="slds-truncate" title="Add Comments">                        
                                        <div class="slds-form-element">
                                            <span class="slds-form-element__label">Add Comments</span>
                                            <div class="slds-form-element__control slds-border_bottom">
                                                <ui:inputTextarea aura:id="addComments" value="" disabled="true" rows="1" cols="50"/>                                                          
                                            </div>
                                        </div>
                                    </div>                                        
                                </td>
                                
                            </tr>
                        </tbody>
                    </table>		
                    <table class="slds-table slds-table_bordered slds-no-row-hover slds-table_cell-buffer">
                        <tbody>
                            <tr>
                                <th scope="row">
                                    <div class="slds-truncate" title="Offer Number">                        
                                        <div class="slds-form-element">
                                            <span class="slds-form-element__label">Active Offer Number</span>
                                            <div class="slds-form-element__control slds-border_bottom">
                                                <span class="slds-form-element__static">{!v.wrapper.offerNumber}</span>
                                            </div>
                                        </div>
                                    </div>
                                </th>
                                
                                <th scope="row">
                                    <div class="slds-truncate" title="Opportunity Name">                        
                                        <div class="slds-form-element">
                                            <span class="slds-form-element__label">Opportunity Name</span>
                                            <div class="slds-form-element__control slds-border_bottom">
                                                <span class="slds-form-element__static">{!v.wrapper.opportunityName}</span>
                                            </div>
                                        </div>
                                    </div>
                                </th>
                                
                                <th scope="row">
                                    <div class="slds-truncate" title="Customer">                        
                                        <div class="slds-form-element">
                                            <span class="slds-form-element__label">Customer</span>
                                            <div class="slds-form-element__control slds-border_bottom">
                                                <span class="slds-form-element__static">{!v.wrapper.customer}</span>
                                            </div>
                                        </div>
                                    </div>
                                </th>
                                
                                <th scope="row">
                                    <div class="slds-truncate" title="Customer">                        
                                        <div class="slds-form-element">
                                            <span class="slds-form-element__label">Market/Country</span>
                                            <div class="slds-form-element__control slds-border_bottom">
                                                <span class="slds-form-element__static">{!v.wrapper.marketOrCountry}</span>
                                            </div>
                                        </div>
                                    </div>
                                </th>
                                
                            </tr>
                            <!--BP-FT-1961-->
                            <tr>
                                <th scope="row">
                                    <div class="slds-truncate" title="WTD Case">                        
                                        <div class="slds-form-element">
                                            <span class="slds-form-element__label">WTD Case</span>
                                            <div class="slds-form-element__control slds-border_bottom">
                                                <!--span class="slds-form-element__static">{!v.wrapper.WinTheDealLink}</span-->
                                                <p><lightning:formattedUrl value="{!v.wrapper.WinTheDealLink}" tooltip="Redirect for Case in Win the Deal Tool" label="{!v.wrapper.WinTheDealLink}" target="_blank" /></p>
                                                <!--a href="{!v.wrapper.WinTheDealLink}" target="_blank">{!v.wrapper.WinTheDealLink}</a-->
                                            </div>
                                        </div>
                                    </div>
                                </th>
                            </tr>
                        </tbody>
                    </table> 
                </div>
            </div>
        </article>
        
        
        
        <article class="slds-card">
            <div class="slds-box">
                <div class="slds-card__header slds-grid">
                    <header class="slds-media slds-media_center slds-has-flexi-truncate">
                        <div class="slds-media__figure">	  
                            <span class="slds-icon_container slds-icon-standard-contact" title="description of icon when needed">
                                <c:svgIcon svgPath="/resource/SldsNew/assets/icons/utility-sprite/svg/symbols.svg#description" class="slds-icon slds-icon_small"/>                                                                         
                            </span>
                        </div>
                        <div class="slds-media__body">
                            <h2>          
                                <span class="slds-text-heading_small">Approval Summary</span>          
                            </h2>                        
                        </div>                    
                        <div class="slds-media__figure slds-grid slds-grid_align-end" style="margin-left: 550px;">
                            <span class="slds-avatar slds-avatar_large  slds-grid slds-grid_align-end">
                                <img alt="Person name" src="{!v.userPicURL}" title="User avatar"/>
                            </span>
                        </div>                        
                        <div class="slds-media__body">
                            
                            <h2>          
                                <span class="slds-text-heading_small slds-grid slds-grid_align-end">Approval Requester: {!v.wrapper.approvalRequester}</span>          
                            </h2>
                        </div>
                    </header>                
                </div>
                
                <div class="slds-card__body">
                    <aura:if isTrue="{!v.strAppComments}">
                        <table class="slds-table slds-table_bordered slds-no-row-hover slds-table_cell-buffer"> 
                            <thead>
                                <th><h3>Approval Requester Comments:</h3></th>
                            </thead>
                            <tbody>                            
                                <tr>                
                                    <th scope="row">
                                        <div class="slds-truncate" title="Offer Value">                        
                                            <div class="slds-form-element">
                                                <div class="slds-form-element__control slds-border_bottom">
                                                    <span class="slds-form-element__static">{!v.strAppComments}</span>
                                                </div>
                                            </div>
                                        </div>
                                    </th>   
                                </tr>                        
                            </tbody>
                        </table>
                    </aura:if>
                    &nbsp;&nbsp;&nbsp;&nbsp;	
                    <table class="slds-table slds-table_bordered slds-no-row-hover slds-table_cell-buffer"> 
                        <thead>
                            <th><h3>LoA Information:</h3></th>
                        </thead>
                        <tbody>                            
                            <tr>                
                                <th scope="row">
                                    <div class="slds-truncate" title="Offer Net Price">                        
                                        <div class="slds-form-element">
                                            <span class="slds-form-element__label">Offer Net Price (CNP)</span>
                                            <div class="slds-form-element__control slds-border_bottom">
                                                <span class="slds-form-element__static">{!v.wrapper.offerValue_CNP}</span>
                                            </div>
                                        </div>
                                    </div>
                                </th>
                                <th scope="row">
                                    <div class="slds-truncate" title="Accounting Value">                        
                                        <div class="slds-form-element">
                                            <span class="slds-form-element__label">Accounting Value (NS)</span>
                                            <div class="slds-form-element__control slds-border_bottom">
                                                <span class="slds-form-element__static">{!v.wrapper.offerValue_NS}</span>
                                            </div>
                                        </div>
                                    </div>
                                </th>
                            </tr>                        
                        </tbody>
                    </table>
                    &nbsp;&nbsp;&nbsp;&nbsp;           
                    <table class="slds-table slds-table_bordered slds-table_col-bordered">
                        <tbody>	
                            <tr>
                                <th scope="row">
                                    <div class="slds-truncate" title="Lead BG">                        
                                        <div class="slds-form-element">
                                            <span class="slds-form-element__label">Lead BG</span>
                                            <div class="slds-form-element__control slds-border_bottom">
                                                <span class="slds-form-element__static">{!v.wrapper.leadBG}</span>
                                            </div>
                                        </div>
                                    </div>
                                </th> 
                                <th scope="row">
                                    <div class="slds-truncate" title="Lead BG">                        
                                        <div class="slds-form-element">
                                            <span class="slds-form-element__label">Lead BU</span>
                                            <div class="slds-form-element__control slds-border_bottom">
                                                <span class="slds-form-element__static">{!v.wrapper.leadBU}</span>
                                            </div>
                                        </div>
                                    </div>
                                </th>
                                <th scope="row">
                                    <div class="slds-truncate" title="Supporting BGs/BUs">                        
                                        <div class="slds-form-element">
                                            <span class="slds-form-element__label">Supporting BGs/BUs</span>
                                            <div class="slds-form-element__control slds-border_bottom">
                                                <span class="slds-form-element__static">{!v.wrapper.supportingBGsBUs}</span>
                                            </div>
                                        </div>
                                    </div>
                                </th>                                       
                                <th scope="row">
                                    <div class="slds-truncate" title="LoA Approval Level">                        
                                        <div class="slds-form-element">
                                            <span class="slds-form-element__label">LoA Approval Level</span>
                                            <div class="slds-form-element__control slds-border_bottom">
                                                <span class="slds-form-element__static">{!v.wrapper.loaApprovalLevel}</span>
                                            </div>
                                        </div>
                                    </div>
                                </th>
                                <th scope="row">
                                    <div class="slds-truncate" title="LoA Level before STA">                        
