                                            <span class="slds-form-element__label">LoA Level before STA</span>
                                            <div class="slds-form-element__control slds-border_bottom">
                                                <span class="slds-form-element__static">{!v.wrapper.loaApprovalLevelBeforeSTA}</span>
                                            </div>
                                        </div>
                                    </div>
