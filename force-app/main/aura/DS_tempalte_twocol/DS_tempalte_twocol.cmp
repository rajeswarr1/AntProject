<aura:component implements="forceCommunity:layout" description="Custom Content Layout" access="global">
    <aura:attribute name="column1left" type="Aura.Component[]" required="false" />
	<aura:attribute name="column2right" type="Aura.Component[]" required="false" />
	
    <lightning:layoutItem size="12" class="page-content" padding="around-small">
   		<lightning:layout multipleRows="true" horizontalAlign="spread">
    		<lightning:layoutItem size="12" class="" mediumDeviceSize="9" largeDeviceSize="9">
        		{!v.column1left}
        	</lightning:layoutItem>
            <lightning:layoutItem size="12" class="" mediumDeviceSize="3" largeDeviceSize="3">
        		{!v.column2right}
        	</lightning:layoutItem>
    	</lightning:layout>
    </lightning:layoutItem>
    
    
           
</aura:component>