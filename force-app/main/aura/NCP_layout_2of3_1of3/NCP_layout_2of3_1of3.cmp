<aura:component implements="forceCommunity:layout" description="Custom full width layout with first column 2-of-3 and second column 1-of-3" access="global">

    <!-- <aura:attribute name="breadcrumbs" type="Aura.Component[]" required="false"></aura:attribute> -->
    <aura:attribute name="contentOne" type="Aura.Component[]" required="false"></aura:attribute>
    <aura:attribute name="contentTwo" type="Aura.Component[]" required="false"></aura:attribute>
    <!-- <div class="slds-p-horizontal_xxx-large slds-p-bottom_medium">
        {!v.breadcrumbs}
    </div> -->
    <div class="slds-grid slds-wrap slds-rp-horizontal_xxx-large slds-grid_pull-padded-small slds-p-bottom_medium slds-p-top_medium">
        <div class="slds-col_padded slds-size_1-of-1 slds-medium-size_2-of-3">{!v.contentOne}</div>
        <div class="slds-col_padded slds-size_1-of-1 slds-medium-size_1-of-3">{!v.contentTwo}</div>
    </div>
</aura:component>