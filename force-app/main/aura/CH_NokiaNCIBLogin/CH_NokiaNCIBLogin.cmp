<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,force:lightningQuickAction,lightning:isUrlAddressable,forceCommunity:availableForAllPageTypes" access="global" >
    <lightning:workspaceAPI aura:id="workspace"/>
    <aura:attribute name="recordId" type="Id"/>
    <aura:attribute name="ncibUrl" type="String"/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
	<iframe width="1500" height="710" src="{!v.ncibUrl}" frameborder="0" sandbox="allow-same-origin allow-scripts allow-forms allow-popups  allow-popups-to-escape-sandbox"></iframe>
</aura:component>