<aura:component controller="NOKIACPQ_LineItemInsertWithAPI" implements="flexipage:availableForAllPageTypes" access="global" >  
    <aura:attribute name="isSuccess" type="boolean" />
    <aura:attribute name="message" type="String" />
    <aura:attribute access="public" name="messageReceived" type="String" default=""/>
    <ltng:require scripts="{!$Resource.NOKIACPQ_NVUIStaticResource + '/jquery-3.3.1.min.js'}" afterScriptsLoaded="{!c.controllerfunctionName}" />
    <aura:attribute type="string" name="configId" default="a6856000000E8VBAA0"/>
    <aura:attribute type="string" name="lineitemid" default="a6856000000E8VBAA0"/>
    <aura:registerevent name="componentEventFired" type="c:NOKIACPQ_MWNetworkViewEvent"></aura:registerevent> <!--Event is being register in child component-->
    <aura:handler name="componentEventFired" event="c:NOKIACPQ_MWNetworkViewEvent" action="{!c.handleMessage1}"></aura:handler><!--Handler for custom event-->
    <aura:attribute name="Spinner" type="boolean" default="false"/>
    
    
    
    <lightning:card >
        <div aura:id="ModalDiv">
            {!v.body}    
        </div>
        <aura:if isTrue="{!v.Spinner}">
            <div aura:id="spinnerId" class="slds-spinner_container">
                <div class="slds-spinner--brand  slds-spinner slds-spinner--large slds-is-relative" role="alert">
                    <span class="slds-assistive-text">Loading</span>
                    <div class="slds-spinner__dot-a"></div>
                    <div class="slds-spinner__dot-b"></div>
                </div>
            </div>
        </aura:if>
        
        
        <div style="height:90vh">
            
            <lightning:container aura:id="vueApp" src="{!$Resource.NOKIACPQ_NVUIStaticResource + '/index.html'}"
                                 onmessage="{!c.handleMessage}"
                                 onerror="{!c.handleError}"/>
        </div>
        <lightning:button class="slds-align_absolute-center" variant="brand" label="Back to Cart"  title="Brand action" onclick="{! c.RedirectTocart }" />
    </lightning:card>    
    
</aura:component>