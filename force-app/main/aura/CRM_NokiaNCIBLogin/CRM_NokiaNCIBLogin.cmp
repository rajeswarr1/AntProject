<aura:component controller="CRM_NokiaNCIBLoginCtlr" implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,force:lightningQuickAction,lightning:isUrlAddressable,forceCommunity:availableForAllPageTypes" access="global" >
    <aura:attribute name="recordId" type="Id"/>
    <aura:attribute name="ncibUrl" type="String"/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
