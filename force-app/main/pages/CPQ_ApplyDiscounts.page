<apex:page controller="CPQ_ApplyDiscountsController" sidebar="false" showHeader="false" standardStylesheets="false">
    <apex:includeLightning />
    <div id="Con"/>
    <script>
    	var quoteId = "{!$CurrentPage.parameters.businessObjectId}";//quoteId
        var confgId_CPQTempObj = "{!$CurrentPage.parameters.configRequestId}";//configId - Temp Object (CPQ)
        var cartId = "{!$CurrentPage.parameters.Id}";//cartId
        var flow = "{!$CurrentPage.parameters.flow}";
    
        $Lightning.use("c:CPQ_BOMUploadApp", function() {
            $Lightning.createComponent( "c:CPQ_ApplyDiscounts", { "quoteId" : quoteId,  "confgId_CPQTempObj" : confgId_CPQTempObj, "cartId" : cartId, "flow" : flow}, "Con",
                function(cmp) {
                    console.log("Component is created!");
                });
            });        
      </script>
</apex:page>